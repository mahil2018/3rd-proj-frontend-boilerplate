<ul>

    {{#each authors }}
    <li>
        <h2>{{this.firstName}} {{this.lastName}}</h2>
        <h3>Nationality: {{this.nationality}}</h3>
         <h3>Born: {{this.birthdate}}</h3>
         <h3>Description: {{this.description}}</h3>
        <img src="{{this.image_url}}" alt="" width="200">    
    </li>
    {{/each}}
</ul>